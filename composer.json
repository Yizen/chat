{
    "name": "Yizen/chat",
    "description": "Chat Package for Laravel 5.4 and up",
    "type": "library",
    "keywords": [
        "laravel",
        "chat",
        "messaging",
        "conversation"
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "predis/predis": "^2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|6.2|^7.0",
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0|~3.7.0|~3.8.0",
        "orchestra/database": "~3.3.0|~3.4.2|^3.5.0|~3.7.0|~3.8.0",
        "mockery/mockery": "^1.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yizen",
            "email": "ogrenet@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Yizen\\Chat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yizen\\Chat\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yizen\\Chat\\ChatServiceProvider"
            ],
            "aliases": {
                "Chat": "Yizen\\Chat\\Facades\\ChatFacade::class"
            }
        }
    }
}
